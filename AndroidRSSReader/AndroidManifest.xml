<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.msi.androidrss" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="4" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application android:icon="@drawable/washlogo" >
        <activity
            android:name=".RSSReader"
            android:label="Sen Leanna W."
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowDescription"
            android:screenOrientation="portrait" >
            <intent-filter >
                <action android:name="android.intent.start.main"/>
            </intent-filter>
        </activity>
        <activity android:name=".ShowDescriptiontwo"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".RSSReadertwo"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".mainpick"
            android:screenOrientation="portrait" >
            <intent-filter >
                <action android:name="android.intent.start.realmain"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>